GET     /health                           controllers.HomeController.health()
POST    /urls                             controllers.UrlController.addUrl()
GET     /urls                             controllers.UrlController.getUrls()
GET     /urls/:shortCode                  controllers.UrlController.getUrlByShortCode(shortCode: String)
DELETE  /urls/:shortCode                  controllers.UrlController.deleteUrlByShortCode(shortCode: String)
GET     /notifications                    controllers.UrlController.getNotifications()
POST    /user                             controllers.UserController.addUser()
POST    /auth/login                       controllers.AuthController.login()
GET    /:shortCode                        controllers.UrlController.redirectUrl(shortCode: String)
